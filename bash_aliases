
# POWER-ON
alias SW_OFF='sudo sispmctl -D 01:01:50:be:53 -f 1'
alias FW_OFF='sudo sispmctl -D 01:01:50:be:53 -f 2'
alias R1_OFF='sudo sispmctl -D 01:01:50:be:53 -f 3'
alias P4_OFF='sudo sispmctl -D 01:01:50:be:53 -f 4'

# POWER-ON
alias SW_ON='sudo sispmctl -D 01:01:50:be:53 -o 1'
alias FW_ON='sudo sispmctl -D 01:01:50:be:53 -o 2'
alias R1_ON='sudo sispmctl -D 01:01:50:be:53 -o 3'
alias P4_ON='sudo sispmctl -D 01:01:50:be:53 -o 4'

